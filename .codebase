198 | // mapToStruct converts a map[string]interface{} to a struct using JSON marshaling.
199 | func mapToStruct(m map[string]interface{}, v interface{}) error {
200 | 	b, err := json.Marshal(m)
201 | 	if err != nil {
202 | 		return err
203 | 	}
204 | 	return json.Unmarshal(b, v)
205 | }
