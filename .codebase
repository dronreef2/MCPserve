 1 | import os
 2 | from openai import OpenAI
 3 | 
 4 | model = "gemini-2.0-flash"
 5 | 
 6 | 
 7 | def call_openai(prompt: str, content: str) -> str:
 8 |     api_key = os.getenv("GOOGLE_AI_STUDIO_KEY")
 9 |     base_url = os.getenv("OPENAI_BASE_URL", "https://gemini2gpt.programnotes.cn/v1")
10 |     client = (
11 |         OpenAI(api_key=api_key, base_url=base_url)
12 |         if base_url
13 |         else OpenAI(api_key=api_key)
14 |     )
15 |     response = client.chat.completions.create(
16 |         model=model,
17 |         messages=[
18 |             {"role": "system", "content": prompt},
19 |             {"role": "user", "content": content},
20 |         ],
21 |         max_tokens=100000,
22 |         temperature=0.7,
23 |         timeout=600,
24 |     )
25 |     return response.choices[0].message.content.strip()
26 | 


