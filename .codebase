/Dockerfile:
--------------------------------------------------------------------------------
 1 | FROM python:3.12-bookworm AS builder
 2 | 
 3 | RUN apt-get update && apt-get install -y --no-install-recommends python3-venv curl
 4 | 
 5 | ENV PATH="/root/.local/bin:$PATH"
 6 | 
 7 | RUN curl -sSL https://astral.sh/uv/install.sh | sh && . /root/.profile
 8 | 
 9 | # 设置工作目录
10 | WORKDIR /app
11 | 
12 | # 复制项目依赖声明文件（uv 需要 pyproject.toml 或 requirements.txt）
13 | COPY pyproject.toml uv.lock ./
14 | 
15 | # 安装项目依赖（使用 uv sync 或 uv pip）
16 | RUN uv sync --no-install-project
17 | 
18 | # 生产阶段
19 | FROM python:3.12-bookworm
20 | COPY --from=builder /app/.venv /app/.venv
21 | ENV PATH="/app/.venv/bin:$PATH"
22 | COPY . .
23 | EXPOSE 8000
24 | CMD ["python", "main.py"]


--------------------------------------------------------------------------------
/Dockerfile-go:
--------------------------------------------------------------------------------
 1 | FROM golang:1.23-alpine AS builder
 2 | 
 3 | WORKDIR /app
 4 | 
 5 | COPY go.mod go.sum ./
 6 | RUN go env -w GOPROXY="https://goproxy.io,direct" && go mod download
 7 | 
 8 | COPY . .
 9 | 
10 | RUN go build -o main .
11 | 
12 | FROM alpine:latest
13 | 
14 | WORKDIR /app
15 | 
16 | COPY --from=builder /app/main .
17 | 
18 | EXPOSE 8080
19 | 
20 | CMD ["./main"]
21 | 


