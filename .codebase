116 | func helloHandler(ctx context.Context, request mcp.CallToolRequest) (*mcp.CallToolResult, error) {
117 | 	name, ok := request.Params.Arguments["name"].(string)
118 | 	if !ok {
119 | 		return nil, errors.New("name must be a string")
120 | 	}
121 | 
122 | 	return mcp.NewToolResultText(fmt.Sprintf("Hello, %s!", name)), nil
123 | }
124 | 
125 | func fetchHandler(ctx context.Context, request mcp.CallToolRequest) (*mcp.CallToolResult, error) {
126 | 	var args FetchArgs
127 | 	if err := mapToStruct(request.Params.Arguments, &args); err != nil {
128 | 		return nil, fmt.Errorf("invalid arguments: %w", err)
129 | 	}
130 | 
131 | 	apiKey := os.Getenv("JINA_API_KEY")
132 | 	if apiKey == "" {
133 | 		return nil, errors.New("JINA_API_KEY is not set")
134 | 	}
135 | 
136 | 	client := &http.Client{}
137 | 	req, err := http.NewRequest("GET", fmt.Sprintf("https://r.jina.ai/%s", args.URL), nil)
138 | 	if err != nil {
139 | 		return nil, fmt.Errorf("failed to create request: %w", err)
140 | 	}
141 | 	req.Header.Set("Authorization", "Bearer "+apiKey)
142 | 
143 | 	resp, err := client.Do(req)
144 | 	if err != nil {
145 | 		return nil, fmt.Errorf("failed to make request: %w", err)
146 | 	}
147 | 	defer resp.Body.Close()
148 | 
149 | 	body, err := io.ReadAll(resp.Body)
150 | 	if err != nil {
151 | 		return nil, fmt.Errorf("failed to read response body: %w", err)
152 | 	}
153 | 
154 | 	if resp.StatusCode >= 400 {
155 | 		return nil, fmt.Errorf("API error: %s", string(body))
156 | 	}
157 | 
158 | 	return mcp.NewToolResultText(string(body)), nil
159 | }
160 | 
161 | func searchHandler(ctx context.Context, request mcp.CallToolRequest) (*mcp.CallToolResult, error) {
162 | 	var args SearchArgs
163 | 	if err := mapToStruct(request.Params.Arguments, &args); err != nil {
164 | 		return nil, fmt.Errorf("invalid arguments: %w", err)
165 | 	}
166 | 
167 | 	apiKey := os.Getenv("JINA_API_KEY")
168 | 	if apiKey == "" {
169 | 		return nil, errors.New("JINA_API_KEY is not set")
170 | 	}
171 | 
172 | 	client := &http.Client{}
173 | 	req, err := http.NewRequest("GET", fmt.Sprintf("https://s.jina.ai/?q=%s", args.Query), nil)
174 | 	if err != nil {
175 | 		return nil, fmt.Errorf("failed to create request: %w", err)
176 | 	}
177 | 	req.Header.Set("Authorization", "Bearer "+apiKey)
178 | 	req.Header.Set("X-Respond-With", "no-content")
179 | 
180 | 	resp, err := client.Do(req)
181 | 	if err != nil {
182 | 		return nil, fmt.Errorf("failed to make request: %w", err)
183 | 	}
184 | 	defer resp.Body.Close()
185 | 
186 | 	body, err := io.ReadAll(resp.Body)
187 | 	if err != nil {
188 | 		return nil, fmt.Errorf("failed to read response body: %w", err)
189 | 	}
190 | 
191 | 	if resp.StatusCode >= 400 {
192 | 		return nil, fmt.Errorf("API error: %s", string(body))
193 | 	}
194 | 
195 | 	return mcp.NewToolResultText(string(body)), nil
196 | }
